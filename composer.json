{
  "name": "rollun-com/rollun-utils",
  "description": "Utils for middleware and pipes",
  "type": "library",
  "license": "GPL-3.0",
  "keywords": [
    "php",
    "rest",
    "data store",
    "middleware"
  ],
  "authors": [
    {
      "name": "avz-cmf",
      "email": "email@example.com"
    },
    {
      "name": "victorynox",
      "email": "it.proffesor02@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-stratigility": "^1.1",
    "psr/http-message": "^1.0",
    "zendframework/zend-diactoros": "^1.1",
    "zendframework/zend-config": "^2.6",
    "zendframework/zend-stdlib": "^3",
    "google/apiclient": "2.0",
    "rollun-com/rollun-installer": "dev-master",
    "mindplay/jsonfreeze": "0.3.3.x-dev",
    "opis/closure": "^2.1.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "rollun\\utils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\utils\\": "tests/src"
    }
  },
  "scripts": {
    "lib-install": "rollun\\installer\\Command::install",
    "lib-uninstall": "rollun\\installer\\Command::uninstall",
    "lib-reinstall": "rollun\\installer\\Command::reinstall"
  }
}
